{
  "id": "ppd-s05cdh",
  "name": "PreprocessDesign",
  "version": "1.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "ID",
      "minVersion": "19.4.0"
    }
  ],
  "requiredPermissions": {
    "localFileSystem": "fullAccess"
  },
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "panel",
      "id": "PreProcessDesign",
      "minimumSize": {
        "width": 200,
        "height": 200
      },
      "maximumSize": {
        "width": 400,
        "height": 800
      },
      "preferredDockedSize": {
        "width": 300,
        "height": 600
      },
      "preferredFloatingSize": {
        "width": 300,
        "height": 600
      },
      "label": {
        "default": "PreProcessDesign"
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark.png",
          "scale": [1, 2],
          "theme": ["darkest", "dark", "medium"]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light.png",
          "scale": [1, 2],
          "theme": ["lightest", "light"]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icons/plugin.png",
      "scale": [1, 2],
      "theme": ["darkest", "dark", "medium", "lightest", "light", "all"],
      "species": ["pluginList"]
    }
  ]
}
